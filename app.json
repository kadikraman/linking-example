{
  "expo": {
    "name": "Linking example",
    "slug": "linking-example",
    "scheme": "linking-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "light": "./assets/icon-light.png",
        "dark": "./assets/icon-dark.png",
        "tinted": "./assets/icon-tinted.png"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.expokadi.linking-example",
      "associatedDomains": ["applinks:linking-example.expo.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.expokadi.linkingexample"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "server"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/icon-light.png",
          "dark": {
            "image": "./assets/icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 150
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6f6bd23b-e14a-4c44-b36b-be0ecdca9549"
      }
    },
    "owner": "expokadi"
  }
}
